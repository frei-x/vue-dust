  "private": false,
  "description": "Vue3.0 composition API library",
  "author": {
    "name": "衣沾不足惜",
    "email": "javascript.h@qq.com",
    "url": "https://gitee.com/stringify"
  },
  "keywords": [
    "Vue3",
    "composition-api",
    "composition",
    "VAC",
    "vue",
    "hooks"
  ],
  "bugs": {
    "url": "https://gitee.com/stringify/vue-dust/issues",
    "email": "javascript.h@qq.com"
  },
  "repository": {
    "type": "git",
    "url": "https://gitee.com/stringify/vue-dust"
  },
  "homepage": "https://gitee.com/stringify/vue-dust",
  "main": "dist/vue-dust.umd.min.js",
  "scripts": {
    "serve": "vue-cli-service serve src/index.js",
    "build": "vue-cli-service build --target lib --name vue-dust src/index.js",
    "lint": "vue-cli-service lint",
    "publish-alpha": "npm version prerelease --preid=alpha && npm publish --tag alpha"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "vue": "^3.0.0-rc.10"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.0",
    "@vue/cli-plugin-eslint": "^4.5.4",
    "@vue/cli-service": "^4.5.0",
    "@vue/compiler-sfc": "^3.0.0-beta.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0-alpha.0",
    "vue-cli-plugin-vue-next": "~0.1.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-unused-vars": "warn",
      "vue/no-unused-components": "warn"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}